int x =215;

void setup(){
  size(480,120);
}

void draw(){
  //background(200);  입력하면 사각형이 움직이는 것처럼 보인다.
  if(keyPressed&&(key==CODED)){ //특수한 키들 ALT, CONTROL, SHIFT, UP, DOWN, LEFT, RIGHT
    if(keyCode==LEFT){
      x--;
    }
    else if(keyCode==RIGHT){
      x++;
    }
  }
    rect(x,45,50,50);
}
